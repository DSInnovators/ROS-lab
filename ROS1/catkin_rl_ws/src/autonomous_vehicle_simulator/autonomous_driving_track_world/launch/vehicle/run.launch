<?xml version="1.0"?>
<launch>
  <env name="GAZEBO_MODEL_PATH" value="$(find autonomous_driving_track_world):$(find vehicle_gazebo)"/>
  
  <arg name="verbose" default="false"/>
  <arg name="debug" default="false"/>  
  <arg name="gui" default="true"/>
  

  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
   <arg name="world_name" value="$(find autonomous_driving_track_world)/worlds/road_track.world"/>
   <arg name="use_sim_time" value="true"/>
   <arg name="debug" value="$(arg debug)"/>
   <arg name="verbose" value="$(arg verbose)" />  
   <arg name="gui" value="$(arg gui)" />

 
  </include>

  <node 
    name="spawn_urdf" 
    pkg="gazebo_ros" 
    type="spawn_model"     
    
    args="-file $(find vehicle_gazebo)/models/vehicle.sdf
      -x 10.584
      -y -0.5
      -z 0
      -R 0
      -P 0
      -Y -1.57
      -sdf 
      -model vehicle" >    
    </node>
